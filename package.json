        "vue": "^3.4.27"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.2",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.20",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/tsconfig": "^0.5.1",
        "jest": "^29.7.0",
        "terser": "^5.31.0",
        "ts-jest": "^29.3.0",
        "typescript": "~5.3.3",
        "vite": "^5.2.11",
        "vue-tsc": "^2.0.6"
    }

{
    "name": "template-vue-ts",
    "description": "A Phaser 3 TypeScript project template that demonstrates Vue communication and uses Vite for bundling.",
    "version": "1.2.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-vue-ts.git"
    },
